#defining the function to rename columns
rename_columns <- function(prefix, dataframe){
new_names <- c()
for (i in colnames(dataframe)){
i = paste(prefix, i, sep = "")
new_names <- c(new_names, i)
}
colnames(dataframe) <- new_names
}
#renaming column names of the dataframes
rename_columns("kcal_",kcal_intake)
rename_columns("fat_",fat_intake)
rename_columns("protein_",protein_intake)
